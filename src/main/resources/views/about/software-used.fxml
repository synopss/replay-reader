<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
  xmlns:fx="http://javafx.com/fxml"
  fx:controller="com.synops.replayreader.about.SoftwareUsedWindowController"
  prefHeight="400.0" prefWidth="600.0">
  <VBox spacing="12" AnchorPane.topAnchor="12.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.bottomAnchor="12.0">
    <HBox spacing="20">
      <TableView fx:id="softwareTableView" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
        <placeholder>
          <Label>Test</Label>
        </placeholder>
        <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY_ALL_COLUMNS"/>
        </columnResizePolicy>
        <columns>
          <TableColumn fx:id="softwareColumn" text="Software" reorderable="false"/>
          <TableColumn fx:id="licenseColumn" text="License" reorderable="false"/>
        </columns>
      </TableView>
    </HBox>
    <HBox alignment="CENTER_RIGHT">
      <Button fx:id="closeButton" text="Close" prefWidth="80"/>
    </HBox>
  </VBox>
</AnchorPane>
